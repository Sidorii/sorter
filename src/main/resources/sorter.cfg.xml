<?xml version="1.0" encoding="UTF-8"?>
<runner-config>


    <fillers>
        <strategy id="multiplyArrayCfg">
            <input>
                <properties>
                    <property name="max" value="100"/>
                    <property name="length" value="110"/>
                    <property name="tile" value="37"/>
                </properties>

                <properties>
                    <property name="max" value="1000"/>
                    <property name="length" value="1100"/>
                    <property name="tile" value="307"/>
                </properties>

                <properties>
                    <property name="max" value="10"/>
                    <property name="length" value="10"/>
                    <property name="tile" value="234"/>
                </properties>

                <properties>
                    <property name="max" value="10000"/>
                    <property name="length" value="10000"/>
                    <property name="tile" value="10000"/>
                </properties>
            </input>

            <annotationConfig>true</annotationConfig>
        </strategy>
    </fillers>


    <experiment>
        <strategy id="multiplyArrayCfg"/>

        <analyzers>
            <analyser>
                <class>com.netcracker.trainee.analyzer.SorterTimeAnalyzer</class>
            </analyser>
        </analyzers>

        <sorters auto-scan="true">
            <basePackage>com.netcracker.trainee</basePackage>
        </sorters>
    </experiment>

</runner-config>